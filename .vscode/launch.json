    "version": "0.2.0",
    "configurations": [
        {
            "name": "BURF",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${fileDirname}"
        },
        {
            "name": "decrypt",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/delphi",
            "args": ["decrypt","./testdata/fortune2.pem", "<", "${workspaceFolder}/testdata/priv1.bin)"]
        },
        {
            "name": "pub",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/delphi",
            "args": ["--priv", "${workspaceFolder}/testdata/priv1.bin)", "pub"]
        }
    ]
}